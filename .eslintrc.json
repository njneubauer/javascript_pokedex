{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["error", "always"],
        "semi-spacing": ["error", {"before": false, "after": true}],
        "quotes": ["error", "single"]
    }
}
